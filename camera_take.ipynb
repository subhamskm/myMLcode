    "cam=cv2.VideoCapture(0)\n",
    "if cam.isOpened():\n",
    "    print(\"Camera is ready to take picture..\")\n",
    "    status, frame = cam.read()\n",
    "    cv2.imshow(\"frame1\",frame)\n",
    "    cv2.waitKey(0)\n",
    "    cam.release()\n",
    "else:\n",
    "    print(\"Check your camera drivers..\")\n",
